# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
}

type PersonData {     
  first: String     
  last: String     
  gamerTag: String     
  birthday: Int 
}

type StatBlock {     
  mental: Int 
}

type Player {     
  data: PersonData     
  stats: StatBlock 
}

type Query {
    name: String
}

schema {
    query: Query
}